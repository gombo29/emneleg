{% extends '@happyWeb/Layout/base.html.twig' %}
{% block style %}
{% endblock %}
{% block body %}
    <div class="project-type-background">
        <div class="container" style="text-align: center;margin-bottom: 30px;margin-top: 110px;">


            <div class="row">
                <div class="col-md-12" style="margin-bottom: 30px">
                    <div class="home-header">
                        <h4><i class="fa fa-heartbeat"></i> Цахим эмч</h4>
                    </div>
                    <h5 style="margin-top: 30px; margin-bottom: 30px">Та доорх асуултуудаас сонгоно уу.</h5>
                    <ul>
                        {% for key, q in questions %}
                            <li style="margin: 20px 0"><a
                                        href="{{ path('online_doctor_answers', {'id': q.id ,'isfirst': 1}) }}"
                                        style="font-size: 20px">{{ q.name }}?</a></li>
                        {% endfor %}
                    </ul>

                    <a href="{{ path('online_doctor') }}" style="color: #000"><i class="fa fa-arrow-left"></i> <b>Эхлэл хэсэглүү буцах</b></a>
                </div>

            </div>

        </div>
    </div>
{% endblock %}

