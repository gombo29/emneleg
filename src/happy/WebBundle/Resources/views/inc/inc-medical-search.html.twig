<div class="med-search">
    <div class="col-lg-1 col-md-6 col-sm-12 col-xs-12 med-search-view-type">
    </div>
    <div class="col-lg-3 col-md-6 col-sm-12 col-xs-12 med-search-view-type">
        <h5 style="margin-bottom: 20px;font-size: 40px">Эмнэлгийн хайлт</h5>
        <ul class="nav nav-tabs nav-justified" style="border-bottom:none">
            <li class="nav-item">
                <a class="nav-link active" data-toggle="tab" href="#panel1" role="tab"><i class="fa fa-list"></i> <br>Жагсаалт</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" data-toggle="tab" href="#panel2" role="tab"><i class="fa fa-support"></i> <br>Төрөл</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" data-toggle="tab" href="#panel3" role="tab"><i class="fa fa-map-marker"></i> <br>Map</a>
            </li>
        </ul>
    </div>
    <div class="col-lg-7 col-md-6 col-sm-12 col-xs-12 med-search-type">
        {% for i in medicalType%}
            <div class="form-group med-search-checker">
                <input type="checkbox" class="filled-in" id="checkbox{{ i.id }}">
                <label for="checkbox{{ i.id }}">{{ i.name }}</label>
            </div>
        {% endfor %}

    </div>
</div>