<!DOCTYPE html>
<html lang="en">

<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <title>{% block title %}CMS{% endblock %}</title>
    <link rel="shortcut icon" href="{{ asset('recource/ico.ico') }}" sizes="16x16">
    <link rel="stylesheet" type="text/css" href="{{ asset('stat/bootstrap/dist/css/bootstrap.css') }}"/>
    <link rel="stylesheet" type="text/css" href="{{ asset('stat/metisMenu/dist/metisMenu.css') }}"/>
    <link rel="stylesheet" type="text/css"
          href="{{ asset('stat/startbootstrap-sb-admin-2/dist/css/sb-admin-2.css') }}"/>
    <link rel="stylesheet" type="text/css" href="{{ asset('stat/morrisjs/morris.css') }}"/>
    <link rel="stylesheet" type="text/css" href="{{ asset('stat/font-awesome/css/font-awesome.css') }}"/>
    <link rel="stylesheet" type="text/css" href="{{ asset('stat/jquery-ui/themes/smoothness/jquery-ui.min.css') }}"/>
    <link rel="stylesheet" type="text/css" href="{{ asset('stat/jquery-ui/themes/smoothness/theme.css') }}"/>
    <link rel="stylesheet" type="text/css"
          href="{{ asset('stat/eonasdan-bootstrap-datetimepicker/build/css/bootstrap-datetimepicker.min.css') }}"/>
    {% block style %}{% endblock %}
    <style>
        ul {
            list-style: none;
            padding: 0;
        }
    </style>
    <script src="{{ asset('stat/jquery/dist/jquery.js') }}" type="text/javascript"></script>
    <script src="{{ asset('stat/moment/min/moment.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('stat/bootstrap/dist/js/bootstrap.js') }}" type="text/javascript"></script>
    <script src="{{ asset('stat/eonasdan-bootstrap-datetimepicker/build/js/bootstrap-datetimepicker.min.js') }}"
            type="text/javascript"></script>
    <script src="{{ asset('stat/metisMenu/src/metisMenu.js') }}" type="text/javascript"></script>
    <script src="{{ asset('stat/raphael/raphael.js') }}" type="text/javascript"></script>
    <script src="{{ asset('stat/morrisjs/morris.js') }}" type="text/javascript"></script>
    <script src="{{ asset('stat/startbootstrap-sb-admin-2/dist/js/sb-admin-2.js') }}" type="text/javascript"></script>
    <script src="{{ asset('stat/ckeditor/ckeditor.js') }}" type="text/javascript"></script>
</head>
<body>
<div id="wrapper">
    {% include '@happyCms/inc/navigation.html.twig' %}

    <div id="page-wrapper" style="overflow: scroll">
        <div class="row">
            <div class="col-lg-12">
                <h1 class="page-header">{% block pageheader %}CMS-д тавтай морил!{% endblock %}</h1>
            </div>
        </div>

        {% block body %}{% endblock %}
    </div>
</div>


<script type="text/javascript">
    CKEDITOR.timestamp = '20161010104';
</script>


{% block js %}{% endblock %}

<script type="text/javascript">
    $(document).ready(function () {
        $('input[datetime=picker]').parent().css('position', 'relative');
        $('input[datetime=picker]').datetimepicker({
            format: 'YYYY-MM-DD HH:mm'
        });
    });
</script>
</body>

</html>
